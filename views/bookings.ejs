<!DOCTYPE html>
<html>
  <head>
    <title>All Bookings</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <table>
        <tr>
            <th>Email</th>
            <th>Number of Tickets</th>
            <th>Hero</th>
        </tr>
      
        <% for (var booking of bookings) { %>
            <tr>
                <td><%= booking.email %></td>
                <td><%= booking.numTickets %></td>
                <td><%= booking.superhero %></td>
            </tr>
        <% } %>
     </table>
  </body>
</html>